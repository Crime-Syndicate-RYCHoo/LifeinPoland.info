define([], function() {
    /**
     * Funkcja zwracająca informację o tym, czy zasób jest typu "DomainBox"
     *
     * @param {Object} object zasób typu "core_box"
     *
     * @return {boolean} wynik sprawdzenia
     */
    return function(object) {
        return new RegExp('http://home.pl/types/mailbox/domainbox/').test(object.aps.type);
    };
});
