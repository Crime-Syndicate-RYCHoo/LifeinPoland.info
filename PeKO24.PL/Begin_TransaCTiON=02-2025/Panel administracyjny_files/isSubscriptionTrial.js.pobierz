define([
    'helpers/statuses/subscriptionStatuses'
], function(
    subscriptionStatuses
) {
    /**
     * Funkcja odpowiada za sprawdzenie, czy subskrypcja jest w statusie trial.
     *
     * @param {!number} subscriptionStatusId identyfikator statusu subskrypcji
     *
     * @return {boolean} true, je≈õli subskrypcja jest w statusie trial
     */
    return function(subscriptionStatusId) {
        return subscriptionStatuses['trial'] === subscriptionStatusId;
    };
});
